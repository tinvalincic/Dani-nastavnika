{
    "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
    "plugins": ["import"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": "off"
    }
}
